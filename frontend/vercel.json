{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/public/$1"
        }
    ],
    "headers": [
        {
            "source": "/public/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}